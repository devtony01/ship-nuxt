{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vue",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "strict": true,
    "isolatedModules": true,
    "types": ["node", "vite/client"],
    "composite": true,
    "jsx": "preserve",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}