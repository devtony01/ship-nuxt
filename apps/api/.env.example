#######################################################################
# This is the ".env.example" file. Use this to build your own ".env" file.
#
# This file will be committed to version control, so do NOT add secrets here.
#
# When adding additional environment variables, update the schema in "src/config/index.ts".
#######################################################################

# Application Environment
APP_ENV=development
PORT=3001

# Application URLs
API_URL=http://localhost:3001
WEB_URL=http://localhost:3002

# MySQL Database
DATABASE_URL=mysql://ship_user:ship_password@localhost:3306/ship_nuxt

# Redis
REDIS_URI=redis://localhost:6379
REDIS_ERRORS_POLICY=log

# Admin Key (generate with: openssl rand -base64 32)
ADMIN_KEY=

# Resend (for email testing)
RESEND_API_KEY=

# Mixpanel (optional)
MIXPANEL_API_KEY=

# Cloud Storage (optional)
CLOUD_STORAGE_ENDPOINT=
CLOUD_STORAGE_BUCKET=
CLOUD_STORAGE_ACCESS_KEY_ID=
CLOUD_STORAGE_SECRET_ACCESS_KEY=

# Google OAuth (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=