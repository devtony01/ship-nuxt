{
  "name": "web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "Vue 3 web app",
  "author": "devtony01",
  "license": "MIT",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite --port 3000 --host 0.0.0.0",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --port 3000",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "tsc": "vue-tsc --noEmit --watch",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prettier": "prettier . --write",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@ship-nuxt/app-constants": "workspace:*",
    "@ship-nuxt/app-types": "workspace:*",
    "@ship-nuxt/schemas": "workspace:*",
    "@tanstack/vue-query": "^5.62.7",
    "@tanstack/vue-table": "^8.20.5",
    "@vee-validate/zod": "^4.15.0",
    "axios": "^1.7.7",
    "daisyui": "5.0.50",
    "mixpanel-browser": "^2.55.1",
    "pinia": "^3.0.3",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "4.1.12",
    "vee-validate": "4.15.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-sonner": "2.0.8",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@playwright/test": "1.55.0",
    "@ship-nuxt/eslint-config": "workspace:*",
    "@ship-nuxt/prettier-config": "workspace:*",
    "@ship-nuxt/tsconfig": "workspace:*",
    "@tailwindcss/vite": "4.1.13",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "catalog:",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lint-staged": "catalog:",
    "npm-run-all2": "^8.0.4",
    "playwright": "1.55.0",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vite": "^7.0.6",
    "vite-plugin-node-polyfills": "0.24.0",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "bash -c 'vue-tsc --noEmit'",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  }
}
